[Unit]
Description=Service that starts the MyBot splash screen
Before=local-fs.target
DefaultDependencies=no

[Service]
Type=oneshot
WorkingDirectory=/usr/Fusion/FusionServer/Build/scripts
ExecStart=python /usr/Fusion/FusionServer/Build/scripts/startSplashScreen.py

[Install]
WantedBy=sysinit.target
